{
  "name": "aws-saml-login",
  "version": "0.6.0",
  "description": "A simple cli utility to get temporary AWS credentials via a SAML endpoint",
  "main": "./dist/index.js",
  "bin": {
    "aws-saml-login": "./dist/index.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "start": "ts-node src/index.ts",
    "debug": "node --inspect-brk -r ts-node/register ./src/index.ts",
    "build": "tsc -p .",
    "clean": "rm -rf dist/"
  },
  "author": "Matt Aschmann",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattaschmann/aws-saml-login"
  },
  "license": "ISC",
  "engines": {
    "node": ">=10.12.0"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "3.609.0",
    "colors": "1.4.0",
    "commander": "12.1.0",
    "ini": "4.1.3",
    "puppeteer-core": "22.12.1",
    "readline-sync": "1.4.10"
  },
  "devDependencies": {
    "@types/ini": "4.1.1",
    "@types/readline-sync": "1.4.8",
    "ts-node": "10.9.2",
    "tslint": "5.20.1",
    "typescript": "5.5.3"
  }
}
